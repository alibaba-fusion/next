        {
          "name": "value",
          "description": "选中的值，单选时返回单个值，多选时返回数组",
          "type": {
            "name": "union",
            "value": [
              "String",
              "Array"
            ]
          }
        },
        {
          "name": "data",
          "description": "选中的数据，包括 value 和 label，单选时返回单个值，多选时返回数组，父子节点选中关联时，同时选中，只返回父节点",
