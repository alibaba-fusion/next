  "props": {
    "children": {
      "type": {
        "name": "node"
      },
      "required": false,
      "description": "徽标依托的内容，一般显示在其右上方",
      "docblock": "徽标依托的内容，一般显示在其右上方"
    },
    "count": {
      "type": {
        "name": "union",
        "value": [
          {
            "name": "number"
          },
          {
            "name": "string"
          }
        ]
      },
      "required": false,
      "description": "展示的数字，大于 `overflowCount` 时显示为 `${overflowCount}+`，为 `0` 时默认隐藏",
      "defaultValue": {
        "value": "0",
        "computed": false
      },
      "docblock": "展示的数字，大于 `overflowCount` 时显示为 `${overflowCount}+`，为 `0` 时默认隐藏"
