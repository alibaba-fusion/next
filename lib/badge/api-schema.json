      "type": {
        "name": "union",
        "value": [
          {
            "name": "number"
          },
          {
            "name": "string"
          }
        ]
      },
      "required": false,
      "description": "展示的封顶的数字",
      "defaultValue": {
        "value": "99",
        "computed": false
